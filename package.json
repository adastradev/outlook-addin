{
  "name": "outlook-addin",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/adastradev/outlook-addin.git"
  },
  "license": "MIT",
  "config": {
    "app-to-debug": "outlook",
    "app-type-to-debug": "desktop",
    "dev-server-port": 3000
  },
  "scripts": {
    "build": "webpack -p --mode production --https false --env.bucket_name CONFIG_BUCKET_NAME --env.bucket_key CONFIG_BUCKET_KEY",
    "build:dev": "webpack --mode development --https false --env.bucket_name CONFIG_BUCKET_NAME --env.bucket_key CONFIG_BUCKET_KEY",
    "build-dev": "webpack --mode development --https false && echo . && echo . && echo . && echo Please use 'build:dev' instead of 'build-dev'.",
    "dev-server": "webpack-dev-server --mode development",
    "start": "office-addin-debugging start manifest.xml",
    "start:localhost": "office-addin-debugging start manifest-debug.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "stop:localhost": "office-addin-debugging stop manifest-debug.xml",
    "stop:alpha": "office-addin-debugging stop manifest-alpha.xml",
    "test": "cross-env NODE_ENV=test jest",
    "validate": "office-toolbox validate -m manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.596.0",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "core-js": "^3.1.4",
    "enzyme-async-helpers": "^0.9.1",
    "es6-promise": "^4.2.8",
    "moment": "^2.24.0",
    "office-ui-fabric-react": "^7.10.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^24.0.23",
    "@types/office-js": "^1.0.19",
    "@types/office-runtime": "^1.0.8",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-hot-loader": "^4.1.0",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^3.1.6",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "1.11.2",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-cli": "24.5.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "office-addin-debugging": "^2.4.2",
    "office-addin-dev-certs": "^1.3.2",
    "office-toolbox": "^0.2.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.12.6",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
